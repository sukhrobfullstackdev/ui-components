{
  "name": "@magiclabs/ui-components",
  "version": "1.35.8",
  "description": "ðŸ’… A theme-able library of reusable UI components",
  "author": "Magic Labs <open-source@magic.link>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/magiclabs/ui-components.git"
  },
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/es/index.js",
      "require": "./dist/cjs/index.js"
    },
    "./presets": {
      "types": "./dist/types/presets.d.ts",
      "import": "./dist/es/presets.js",
      "require": "./dist/cjs/presets.js"
    }
  },
  "files": [
    "dist/*"
  ],
  "main": "./dist/cjs/index.js",
  "module": "./dist/es/index.js",
  "types": "./dist/types/index.d.ts",
  "engines": {
    "node": ">=20",
    "pnpm": ">=9"
  },
  "scripts": {
    "commit": "git add --all && git-cz",
    "generate:icons": "tsx ./scripts/generateIcons.ts",
    "preinstall": "npx only-allow pnpm",
    "prepare": "panda codegen && husky",
    "lib:dev": "rimraf dist && rollup -w -c rollup.config.mjs && pnpm lib:buildinfo",
    "lib:build": "rimraf dist && rollup -c rollup.config.mjs && pnpm lib:buildinfo",
    "lib:buildinfo": "panda ship --outfile dist/panda.buildinfo.json",
    "lib:publish": "auto shipit -v",
    "docs:dev": "storybook dev -p 6006",
    "docs:build": "storybook build",
    "docs:test": "playwright install && test-storybook",
    "docs:test:ci": "concurrently -k -s first -n \"SB,TEST\" -c \"magenta,blue\" \"pnpm docs:build --quiet && npx http-server storybook-static --port 6006 --silent\" \"wait-on tcp:6006 && pnpm docs:test\"",
    "lint": "eslint --ext .ts,.tsx ./src",
    "reset": "rimraf node_modules && rimraf styled-system && rimraf dist && rimraf coverage",
    "test": "jest --maxWorkers=50%",
    "test:watch": "jest --watch --maxWorkers=25%",
    "test:ci": "jest --runInBand --passWithNoTests",
    "test:build": "publint --strict",
    "test:tree-shaking": "pnpm lib:build && agadoo"
  },
  "devDependencies": {
    "@auto-it/conventional-commits": "^11.1.6",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@pandacss/dev": "^0.40.1",
    "@pandacss/types": "^0.40.1",
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-url": "^8.0.2",
    "@storybook/addon-a11y": "^8.3.0",
    "@storybook/addon-actions": "^8.3.0",
    "@storybook/addon-console": "^3.0.0",
    "@storybook/addon-designs": "^8.0.2",
    "@storybook/addon-docs": "^8.3.0",
    "@storybook/addon-essentials": "^8.3.0",
    "@storybook/addon-interactions": "^8.3.0",
    "@storybook/addon-links": "^8.3.0",
    "@storybook/manager-api": "^8.3.0",
    "@storybook/react": "^8.3.0",
    "@storybook/react-vite": "^8.3.0",
    "@storybook/test": "^8.3.0",
    "@storybook/test-runner": "^0.18.2",
    "@storybook/theming": "^8.3.0",
    "@svgr/core": "^8.1.0",
    "@svgr/plugin-jsx": "^8.1.0",
    "@svgr/plugin-prettier": "^8.1.0",
    "@svgr/plugin-svgo": "^8.1.0",
    "@swc/core": "^1.7.26",
    "@swc/jest": "^0.2.36",
    "@testing-library/jest-dom": "^6.4.5",
    "@testing-library/react": "^16.0.0",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.2",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/yargs": "^17.0.33",
    "@typescript-eslint/eslint-plugin": "^7.13.0",
    "@typescript-eslint/parser": "^7.13.0",
    "agadoo": "^3.0.0",
    "auto": "^11.1.6",
    "axios": "^1.7.2",
    "builtin-modules": "^4.0.0",
    "chalk": "^5.3.0",
    "commitiquette": "^1.2.1",
    "commitizen": "^4.3.0",
    "concurrently": "^8.2.2",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv": "^16.4.5",
    "eslint": "8.57.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-next": "14.2.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-mdx": "^3.1.5",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-storybook": "^0.8.0",
    "figma-api": "^1.11.0",
    "http-server": "^14.1.1",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.5",
    "p-queue": "^8.0.1",
    "playwright": "^1.44.1",
    "prettier": "^3.3.1",
    "publint": "^0.2.8",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rimraf": "^5.0.7",
    "rollup": "^4.30.1",
    "rollup-plugin-esbuild": "^6.1.1",
    "rollup-plugin-multi-input": "^1.4.1",
    "rollup-plugin-node-externals": "^7.1.2",
    "rollup-plugin-tsc-alias": "^1.1.2",
    "rollup-plugin-typescript2": "^0.36.0",
    "rollup-plugin-visualizer": "^5.12.0",
    "storybook": "^8.3.0",
    "storybook-addon-deep-controls": "^0.6.2",
    "storybook-dark-mode": "^4.0.2",
    "ts-jest": "^29.1.4",
    "ts-node": "^10.9.2",
    "tsx": "^4.15.1",
    "typescript": "^5.4.5",
    "vercel": "^34.2.6",
    "vite-tsconfig-paths": "^4.3.2",
    "wait-on": "^7.2.0",
    "yargs": "^17.7.2"
  },
  "peerDependencies": {
    "@pandacss/dev": "^0.35.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "auto": {
    "plugins": [
      "npm",
      "conventional-commits"
    ]
  },
  "dependencies": {
    "create-slots": "^0.6.1",
    "react-aria": "^3.33.1",
    "react-international-phone": "^4.3.0",
    "react-qrcode-logo": "^3.0.0"
  }
}
